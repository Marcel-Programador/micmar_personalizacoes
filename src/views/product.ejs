<%- include('./partials/head') %>
    <main>
        <section>
            <% for(let title of ms) { %>
            <span>
                <h1>
                    <b><%=title.title_impact_2%></b>
                </h1>
            </span>
            <div id="container-home-produtos-product">
                <% for(let product of pm) { %>
                <div class="home-produtos-product">
                    <h3><%=product.category%></h3>
                    <h2><%=title.title_impact_3%></h2>
                    <span><strong>de </strong>R$:<%=product.salePrice%> Por R$:<%=product.specialPrice%></span>

                    <div class="home-produtos-image-product">
                        <img class="h-produtos-image" src="<%=product.img%>" alt="">
                    </div>
                </div>
                <% } %>
            </div>
            <% } %>
        </section>
    </main>
<%- include('./partials/footer') %>