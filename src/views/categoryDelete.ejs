<%- include('./partials/headLogo') %>
    <main>
        <section>
            <h1>Excluir a categoria</h1>

            <form action="/delete/cat/category/<%=categoryResult.id%>?_method=put" method="post">
                <div class="Product-input">
                    <label for="id">Id:</label><%= categoryResult.id %>
                </div>
                <div>
                    <label class="form-label" for="category_opt_singular-id">Categoria Singular: </label><%=categoryResult.category_opt_singular%>
                </div>
                <div>
                    <label class="form-label" for="category_opt_plural-id">Categoria Plural: </label><%=categoryResult.category_opt_plural%>
                </div>
                <button type="submit" class="btn-btn-primary">Excluir</button>
            </form>
            <a href="/actionChange/cat/categoryActionChange"><button type="submit" class="btn-btn-primary">Voltar</button></a>
        </section>
    </main>
<%- include('./partials/footer') %>